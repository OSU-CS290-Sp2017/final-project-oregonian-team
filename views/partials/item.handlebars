<article class="item">
        <div class="image-container">
          {{#each photos}}
             {{>itemPhoto}}
          {{/each}}
        </div>

        <div class="item-content">
          <p class="item-name">
            <a href="#">{{name}}</a>
          </p>
          <p class="item-location">
            <a href="{{location}}">{{locationName}}</a>
          </p>
          <p class="item-decription">
            {{description}} 
          </p>
          <p class="item-more">
            <a href="#">more</a>
          </p>
        </div>
</article>
